.videowrap{
    overflow-x: hidden;
}
/* .video-container{
    position: fixed;
    top: 0;
} */
.myVideo{
    width: 100%; 
}
.video-image-wrap{
    margin-left: 200rpx;
}
.video-image-list{
    width: 50px;
    height: 50rpx;
}
.video-share{
    width: 50rpx;
    height: 40rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 30rpx;
    margin:0;
    padding:0;
    background-position: center;
    border:none !important;
    background-color: #fff;
    position:static;
}
.video-intro{
    border-bottom:2rpx solid #e3e3e3;
    display: flex;
    padding:14rpx 0 20rpx;
    /* margin-top: 460rpx; */
}
.video-content{
    width: 90%;
    font-size:24rpx;
    line-height:45rpx;
    border-bottom:2rpx solid #e3e3e3;
    color: #868585;
    margin:0 auto;
    padding:30rpx 0;
}
.video-bg{
    margin-top: 20rpx;
    width:100%;
    height:144rpx;
}
.video-keyword{
    display:flex;
    flex-direction:row;
    flex-wrap:wrap;
    width: 96%;
    margin-left: 37rpx;
}
.keyword{
    background-color: rgba(0, 0, 0, 0.04);
    border:1rpx solid #e3e3e3;
    margin-right: 30rpx;
    margin-bottom: 30rpx;
    display:inline-block;
    border-radius: 30rpx;
    font-size: 22rpx;
    padding:10rpx 20rpx; 
}
.recommendations{
    width: 100%;
    display: flex;
    white-space: nowrap;
    height: 200rpx;
    margin-left: 37rpx;
}
.homeImage-video{
    width:310rpx !important;
    height:100% !important;
    margin-right:15rpx;    
}
.recommendations-title{
    font-size:30rpx;
    color:#000;
    margin-bottom:30rpx;
    margin-left:37rpx;
    margin-top:30rpx;
}
.recommendations-wrap{
    border-top:2rpx solid #e3e3e3;
    margin-bottom:115rpx;

}
.video-title{
    font-size:30rpx;
    margin-left:37rpx;
    width: 300rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
}
.barrage{
    z-index: 9999999;
    position: fixed;
    bottom: 0;
    background-color: #fff;
    width: 100%;
    display: flex;
    padding:15rpx 10rpx;
    border-top: 2rpx solid #eeeeee;
    align-items: center;
    /* justify-content: space-around; */
}
.weui-input{
    font-size: 20rpx;
    border:2rpx solid #eeeeee;
    width: 600rpx;
    border-radius: 10rpx;
    padding-left: 30rpx;
    background-color: #f4f4f4;

}
.page-body-button{
    padding:0;
    margin:0;
    font-size: 20rpx;
    margin-left: 15rpx;
    color: #00b38a;
    width: 80rpx;
    height: 52rpx;

}
.myImage{
    width: 100%;
}







