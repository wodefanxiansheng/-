page{
  background-color: #eeeeee;
}
.userinfo {
  widows: 100%;
  height: 346rpx;
  background-color: #fff;
  border-top: 4rpx solid #eeeeee;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  background-color: #eeeeee;
}

.userinfo-nickname {
  color: #000;
  font-size: 28rpx;
}
.userinfo-portrait{
  width:100%;
  display:flex;
  align-items:center;
  justify-content:center;
  flex-direction:column;
  margin-top: 40rpx;
}
.my-list-tip{
  background-color: #fff;
  margin-top: 20rpx;
  font-size: 30rpx;
  /* padding:0 40rpx; */
}
.my-list{
  padding:20rpx 0;
  font-size:27rpx;
  margin-left:15px;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 40rpx;
  background-position: 98% 50%;
}
.my-collect,.my-work{
  border-bottom: 2rpx solid #eeeeee;
}
.sign-btn{
  position: static;
  background-color: #fff;
  font-size: 30rpx;
  margin-top: 20rpx;
  /* color: #b8b8b8; */
}
.service-conversation{
  background-color: #fff !important; 
  margin-top: 20rpx !important;
  font-size: 30rpx;
}

#map{
  width:100%;
  height: 400rpx;
  margin:20rpx 0 50rpx;
}





























